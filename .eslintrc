{
	"root": true,
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module"
	},
	"extends": "airbnb-base",
	"rules": {
		"indent": [2, "tab", {"SwitchCase": 1}],
		"class-methods-use-this": [0],
		"no-tabs": [0],
		"arrow-parens": [0],
		"function-paren-newline": [0],
		"object-curly-newline": [0]
	}
}
